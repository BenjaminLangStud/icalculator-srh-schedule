<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.benny.icalculation.gui.DialogController">
   <children>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="300.0">
         <top>
            <FlowPane alignment="CENTER" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Label fx:id="messageLabel" alignment="CENTER" contentDisplay="CENTER" text="Label">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Label>
                        <Label fx:id="subMessageLabel" text="Label" wrapText="true" />
                     </children>
                  </VBox>
               </children>
               <BorderPane.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </BorderPane.margin>
            </FlowPane>
         </top>
         <bottom>
            <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="20.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button defaultButton="true" mnemonicParsing="false" onAction="#okBtnHandler" text="OK" />
                  <Button mnemonicParsing="false" onAction="#okBtnHandler" text="Also OK" />
               </children>
            </FlowPane>
         </bottom>
      </BorderPane>
   </children>
</Pane>
